function main(){
   goCorner();
}


function goCorner(){
   putBeeper();
   move();
   turnLeft();
   move();
   putBeeper();
   turnRight();
   move();
   turnLeft();
   move();
   putBeeper();
   turnRight();
   move();
   turnLeft();
   move();
   putBeeper();
   turnRight();
   move();
   turnLeft();
   move();
   putBeeper();
  
}
