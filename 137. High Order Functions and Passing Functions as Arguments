
function add(num1, num2) {
    return num1+num2;
}

function multiply(num1, num2) {
    return num1*num2;
}
function subtrack(num1, num2){
    return num1-num2;
}
function divide (num1, num2){
    return num1/num2;
}
function calculator(num1, num2, operator) {  // High Order Functions and Passing Functions as Arguments
    return operator(num1,num2);
}

//also:
debugger;
calculator(3,4, multiply);
