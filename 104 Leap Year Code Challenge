function isLeap(year) {
    
var check4=year%4;
var check100=year%100;
var check400=year%400;

if (check4=0) {
    if (check100=0) {
        if (check400=0) {
            alert("Leap year.");
        }
        else {
            alert("Not leap year.");
        }
        
    } else{
        alert("Not leap year.");
    }
} else {
    alert("Not leap year.");
}
}
isLeap(2400);

