

var developer1 ={
    name: "Thanasis",
    age: 45,
    languages: ["Java", "Python"],
    webPage: function(){                  // anonymous function
        alert("How can I serve you, please?");
        openFrame();
        testWebSite();
    }
}

--------------JS Constructor Function------------------

function Developer(name, age, languages) {
    this.name = name;
    this.age = age;
    this.languages = languages;
    this.webPage= function(){                  // anonymous function
        alert("How can I serve you, please?");
        openFrame();
        testWebSite();
    }
}
